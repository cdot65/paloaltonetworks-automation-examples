# Copy this file to terraform.tfvars and fill in your values
# Do NOT commit terraform.tfvars to version control!

# SCM Authentication
# Option 1: Use individual credentials (not recommended)
# scm_client_id     = "your-client-id-here"
# scm_client_secret = "your-client-secret-here"
# scm_scope         = "your-scope-here"

# Option 2: Use auth file (recommended)
# scm_auth_file = "/path/to/your/scm-auth.json"

# SCM Configuration
scm_host     = "api.sase.paloaltonetworks.com"
scm_auth_url = "https://auth.apps.paloaltonetworks.com/auth/v1/oauth2/access_token"
scm_logging  = "quiet"

# Project Configuration
folder       = "Shared"
environment  = "development"
project_name = "scm-test"
