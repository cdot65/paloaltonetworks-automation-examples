# PAN-OS Firewall Configuration
# Copy this file to .env and fill in your actual values

# Firewall hostname or IP address
PANOS_HOSTNAME=your-firewall-hostname.example.com

# Administrator username
PANOS_USERNAME=admin

# Current administrator password
PANOS_PASSWORD=your-current-admin-password

# Optional: Password complexity settings
# PASSWORD_LENGTH=16
# PASSWORD_USE_UPPERCASE=true
# PASSWORD_USE_LOWERCASE=true
# PASSWORD_USE_DIGITS=true
# PASSWORD_USE_SYMBOLS=true