# PAN-OS Firewall Connection
PANOS_HOSTNAME=192.168.1.1
PANOS_USERNAME=admin
PANOS_PASSWORD=your_password_here

# Optional: API Key (if using API key auth instead of username/password)
# PANOS_API_KEY=your_api_key_here

# Anthropic API Key for LLM
ANTHROPIC_API_KEY=sk-ant-your-key-here

# LangGraph Configuration
LANGGRAPH_API_URL=http://localhost:8000
LANGSMITH_API_KEY=lsv2_redacted
LANGSMITH_TRACING=true
LANGSMITH_PROJECT=panos-agent
LANGSMITH_ENDPOINT=https://api.smith.langchain.com

# Agent Configuration
DEFAULT_MODE=autonomous  # autonomous or deterministic
LOG_LEVEL=INFO
